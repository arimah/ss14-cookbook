{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "declaration": false,
    "sourceMap": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "jsx": "react-jsx"
  },
  "exclude": [
    "bin",
    "public"
  ]
}
